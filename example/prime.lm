let solveDivisor n =
begin
    let divisorList = [];
    for i as num in
        1 ~ (n + 1) do
    begin
        let remainder = n % i;
        if remainder == 0 then
            divisorList += [i]
    end;
    divisorList
end;

print "Primality checker\n";
while let n =
    input ">> " as num loop
begin
    if solveDivisor n == [1, n] then
        print f"TRUE: {n} is prime number\n"
    else
        print f"FALSE: {n} is diviable number\n"
end
